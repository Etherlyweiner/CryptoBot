# Network Configuration
NETWORK=mainnet-beta  # Options: mainnet-beta, testnet, devnet
RPC_URL=https://api.mainnet-beta.solana.com
RPC_TIMEOUT=30

# Trading Configuration
MAX_TRADES=5
POSITION_SIZE=0.1  # Percentage of portfolio per trade
STOP_LOSS_PERCENT=2
TAKE_PROFIT_PERCENT=4

# Technical Analysis Parameters
RSI_PERIOD=14
RSI_OVERBOUGHT=70
RSI_OVERSOLD=30
EMA_FAST=12
EMA_SLOW=26
MACD_SIGNAL=9

# Risk Management
MAX_DRAWDOWN=10  # Maximum drawdown percentage
DAILY_LOSS_LIMIT=5  # Daily loss limit percentage
MAX_POSITION_SIZE=20  # Maximum position size percentage

# Solana Transaction Settings
COMMITMENT_LEVEL=confirmed  # Options: processed, confirmed, finalized
TRANSACTION_TIMEOUT=60  # Seconds
MAX_RETRIES=3
RETRY_DELAY=1  # Seconds between retries

# Jupiter DEX Settings
JUPITER_QUOTE_API=https://quote-api.jup.ag/v4
SLIPPAGE_BPS=50  # 0.5% slippage tolerance
